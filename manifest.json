{
    "manifest_version": 2,
    "name": "InstaGet",
    "version": "1.1.0",
    "description": "Download any Instagram image/video in effortless way! - Updated 2018/08/14",
    "author": "Ho Vu - qwerty22121998@gmail.com",
    "homepage_url": "https://www.facebook.com/khanhvu2212",
    "icons": {
        "16": "icons/icon16.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "downloads",
        "background"
    ],
    "content_security_policy": "script-src 'self'; default-src 'self'",
    "background": {
        "scripts": ["scripts/background.js"]
    },
    "content_scripts": [{
        "matches": [
            "https://www.instagram.com/*"
        ],
        "run_at": "document_end",
        "js": [
            "scripts/jquery.js",
            "scripts/main.js"
        ]
    }],
    "browser_action": {
        "default_title": "InstaGet Plugin"
    }
}